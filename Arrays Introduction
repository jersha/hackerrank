#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    int n, value;
    std::cin >> n;
    int* a = new int[n];
    for (int loop = 0; loop < n; loop++) {
        std::cin >> value;
        a[loop] = value;
    }
    for (int loop = (n - 1); loop >= 0; loop--) {
        std::cout << a[loop] << " ";
    }
    delete[] a;
}
