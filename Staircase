#include <bits/stdc++.h>

using namespace std;

// Complete the staircase function below.
void staircase(int n) {
    for(int loop1 = 0; loop1<n; loop1++){
        for(int loop2 = 0; loop2<n; loop2++){
            if((loop1 + loop2) >= (n-1)){
                std::cout<<"#";
            }else{
                std::cout<<" ";
            }
        }
        std::cout<<std::endl;
    }
}

int main()
{
    int n;
    cin >> n;
    cin.ignore(numeric_limits<streamsize>::max(), '\n');

    staircase(n);

    return 0;
}
